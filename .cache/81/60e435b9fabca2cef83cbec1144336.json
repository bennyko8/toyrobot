.y=this.y+1; \n            }\n        }\n\n        if(this.face=\"South\"){\n            if(this.y-1>=0){\n                this.y=this.y-1; \n            }\n        }\n\n        if(this.face=\"East\"){\n            if(this.x+1<=4){\n                this.x=this.x+1; \n            }\n        }\n\n        if(this.face=\"West\"){\n            if(this.x-1>=0){\n                this.x=this.x-1; \n            }\n        }\n\n    }\n\n    right(){\n\n        if(this.place){\n            if(this.face==\"North\"){\n                this.face=\"East\";\n            }\n            if(this.face==\"East\"){\n                this.face=\"South\";\n            }\n            if(this.face==\"South\"){\n                this.face=\"West\";\n            }\n            if(this.face==\"West\"){\n                this.face=\"North\";\n            }\n        }\n\n    }\n\n    left(){\n\n        if(this.place){\n            if(this.face==\"North\"){\n                this.face=\"West\";\n            } else\n            if(this.face==\"East\"){\n                this.face=\"North\";\n            } else\n            if(this.face==\"South\"){\n                this.face=\"East\";\n            } else \n            if(this.face==\"West\"){\n                this.face=\"South\";\n            }\n        }\n\n    }\n\n}\n\nexport default Robot;"},"lineCount":null}},"error":null,"hash":"657fcd6ceeb87bd7bd62c13b6da369be","cacheData":{"env":{}}}